<div class="col-sm-5 col-md-4 center-block ">

<h2 class="text-muted text-center">Log in</h2>

<%= form_for(resource, as: resource_name,class:"form", url: session_path(resource_name)) do |f| %>


<div class="form-control-wrapper form-group">
  <%= f.email_field :email, autofocus: true , class: "form-control empty" %>
  <div class="floating-label">Username</div>
  <span class="material-input"></span>
</div>
<div class="form-control-wrapper form-group">
  <%= f.password_field :password, autocomplete: "off" ,class: "form-control empty" %>

    <div class="floating-label">Password</div>
  <span class="material-input"></span>
</div>
<% if devise_mapping.rememberable? -%>
<div class="form-control-wrapper">
  <%= f.check_box :remember_me %>
  <%= f.label :remember_me %>
</div>
<% end -%>
<div class="actions center-block">
  <%= f.submit "Sign In" ,class:"btn btn-success btn center-block " %>
</div>
<% end %>

<%= render "users/shared/links" %>
</div>